<?xml version="1.0" encoding="utf-8"?>


<androidx.constraintlayout.widget.ConstraintLayout xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main_app_bar_contents"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@null"
    app:layout_collapseMode="parallax"
    app:layout_constraintBottom_toBottomOf="parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_gravity="center_horizontal">



    <androidx.coordinatorlayout.widget.CoordinatorLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@android:color/white"
       >

        <com.google.android.material.floatingactionbutton.FloatingActionButton
            android:id="@+id/tgaddRoom"
            android:clickable="true"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="bottom|end"
            android:layout_margin="26dp"
            android:src="@drawable/addbtn"
            app:backgroundTint="#79A7FF" />

        <ScrollView
            android:layout_width="match_parent"
            android:layout_height="match_parent">
        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="900dp">

        <ImageView
            android:id="@+id/tgimg"
            android:layout_width="wrap_content"
            android:layout_height="270dp"
            android:layout_gravity="top|center"
            android:src="@drawable/studytogether" />

        <TextView
            android:id="@+id/tgcopy"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text = "STUDY TOGETHER"
            android:layout_below="@+id/tgimg"
            android:gravity="center"
            android:textSize="24dp"
            android:layout_marginTop="10dp"
            android:fontFamily="@font/typo"/>

            <ImageView
                android:layout_width="350dp"
                android:layout_height="40dp"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="20dp"
                android:layout_below="@+id/tgcopy"
                android:background= "@drawable/search"
                />

            <EditText
                android:id="@+id/tgtag"
                android:layout_width="300dp"
                android:layout_height="wrap_content"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="30dp"
                android:layout_below="@+id/tgcopy"
                android:hint="#태그검색"
                android:textSize="16dp"
                android:fontFamily="@font/typo"
                android:background="@android:color/transparent"
                />


            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:layout_below="@+id/tgtag">

                <de.hdodenhof.circleimageview.CircleImageView
                    android:id="@+id/room1"
                    android:layout_width="100dp"
                    android:layout_height="100dp"
                    android:layout_marginTop="32dp"
                    android:src="@drawable/roompic"
                    app:civ_border_color="#FF000000"
                    app:civ_border_width="0dp" />

                <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/room1"
                android:text = "#회계사"
                android:fontFamily="@font/typo"
                android:textSize="13dp"
                android:layout_marginTop="7dp"
                android:textColor="@android:color/black"
                />

            <de.hdodenhof.circleimageview.CircleImageView
                android:id="@+id/room2"
                android:layout_toRightOf="@+id/room1"
                android:layout_marginLeft="38dp"
                android:layout_marginTop="32dp"
                android:layout_width="100dp"
                android:layout_height="100dp"
                android:layout_gravity="center_vertical"
                app:civ_border_color="#FF000000"
                app:civ_border_width="0dp"
                android:src="@drawable/roompic"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/room2"
                android:layout_marginLeft="138dp"
                android:fontFamily="@font/typo"
                android:text="#사진기능사"
                android:textColor="@android:color/black"
                android:layout_marginTop="7dp"
                android:textSize="13dp" />

            <de.hdodenhof.circleimageview.CircleImageView
                android:id="@+id/room3"
                android:layout_marginTop="32dp"
                android:layout_toRightOf="@+id/room2"
                android:layout_marginLeft="38dp"
                android:layout_width="100dp"
                android:layout_height="100dp"
                android:layout_gravity="center_vertical"
                app:civ_border_color="#FF000000"
                app:civ_border_width="0dp"
                android:src="@drawable/roompic"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/room3"
                android:text = "#토익모의고사"
                android:fontFamily="@font/typo"
                android:textSize="13dp"
                android:layout_marginLeft="277dp"
                android:layout_marginTop="7dp"
                android:textColor="@android:color/black"
                />

            <de.hdodenhof.circleimageview.CircleImageView
                android:id="@+id/room4"
                android:layout_width="100dp"
                android:layout_height="100dp"
                android:layout_below="@+id/room3"
                android:layout_marginTop="32dp"
                android:src="@drawable/roompic"
                app:civ_border_color="#FF000000"
                app:civ_border_width="0dp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/room4"
                android:text = "#정보처리기사"
                android:fontFamily="@font/typo"
                android:textSize="13dp"
                android:layout_marginTop="7dp"
                android:textColor="@android:color/black"
                />


            <de.hdodenhof.circleimageview.CircleImageView
                android:id="@+id/room5"
                android:layout_below="@+id/room2"
                android:layout_marginTop="32dp"
                android:layout_toRightOf="@+id/room4"
                android:layout_marginLeft="38dp"
                android:layout_width="100dp"
                android:layout_height="100dp"
                android:layout_gravity="center_vertical"
                app:civ_border_color="#FF000000"
                app:civ_border_width="0dp"
                android:src="@drawable/roompic"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/room5"
                android:text = "#토익기초"
                android:fontFamily="@font/typo"
                android:textSize="13dp"
                android:layout_marginLeft="138dp"
                android:layout_marginTop="7dp"
                android:textColor="@android:color/black"
                />

            <de.hdodenhof.circleimageview.CircleImageView
                android:id="@+id/room6"
                android:layout_below="@+id/room3"
                android:layout_marginTop="32dp"
                android:layout_toRightOf="@+id/room5"
                android:layout_marginLeft="38dp"
                android:layout_width="100dp"
                android:layout_height="100dp"
                android:layout_gravity="center_vertical"
                app:civ_border_color="#FF000000"
                app:civ_border_width="0dp"
                android:src="@drawable/roompic"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/room6"
                android:text = "#컴퓨터활용능력"
                android:fontFamily="@font/typo"
                android:textSize="13dp"
                android:layout_marginLeft="277dp"
                android:layout_marginTop="7dp"
                android:textColor="@android:color/black"
                />

        </RelativeLayout>
        </RelativeLayout>
        </ScrollView>
    </androidx.coordinatorlayout.widget.CoordinatorLayout>
</androidx.constraintlayout.widget.ConstraintLayout>